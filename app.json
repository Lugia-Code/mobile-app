{
  "expo": {
    "name": "Tracking Code",
    "slug": "Tracking-code",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/trackingcode-icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      "expo-notifications"
    ],
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/trackingcode-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/trackingcode-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}